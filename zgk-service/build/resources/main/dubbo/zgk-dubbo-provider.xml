<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans        http://www.springframework.org/schema/beans/spring-beans.xsd        http://code.alibabatech.com/schema/dubbo        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <description>gaokao dubbo 服务提供配置 </description>

    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="gk"/>

    <dubbo:registry address="${dubbo_zk_url}" file="/tmp/dubbo.cache"/>

    <dubbo:protocol name="dubbo" port="-1" />

    <dubbo:reference id="answerService" interface="cn.thinkjoy.ss.api.IAnswerService" check="false" retries="0" timeout="600000"/>

    <dubbo:reference id="questionService" interface="cn.thinkjoy.ss.api.IQuestionService" check="false" retries="0" timeout="600000"/>

    <dubbo:reference id="userAccountService" interface="cn.thinkjoy.ss.api.IUserAccountService" check="false" retries="0" timeout="600000"/>

    <dubbo:reference id="userInfoService" interface="cn.thinkjoy.ss.api.IUserInfoService" check="false" retries="0" timeout="600000"/>

</beans>