<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.thinkjoy.gk.dao.IPolicyInterpretationExDAO">


    <select id="findPolicyInterpretationCategorysByBatchIdAndProvinceId" resultType="PolicyInterpretation">
        SELECT
        id,
        categoryName
        FROM gk_policy_interpretation WHERE
        admissionBatchId = #{batchId}
        AND
        provinceId = #{provinceId}
        AND
        status = 1
        <if test="areaId != null">
            AND areaId = #{areaId}
        </if>
    </select>



</mapper>

